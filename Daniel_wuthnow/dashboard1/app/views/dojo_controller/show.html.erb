<%= @Dojo.branch %>
<p>Address: <%= @Dojo.street %></p>
<p>City: <%= @Dojo.city %></p>
<p>State: <%= @Dojo.state %></p>
<h2>Student Roster</h2>
<table>
	<tr>
		<th>First Name</th>
		<th>Last Name</th>
		<th>Email</th>
		<th>Actions</th>
	</tr>
	<% @Student.each do |student| %>
	<tr>
	  <td><%= student.first_name %></td>
	  <td><%= student.last_name %></td>
	  <td><%= student.email %></td>
	  <td><a href="/dojos/<%= @Dojo.id %>/students/<%= student.id %>">Show</a> | <a href="/dojos/<%= @Dojo.id %>/students/<%= student.id %>/edit">Edit</a> | <a href="">Destroy</a></td>
	  </tr>
	<% end %>

</table>
<a href="/dojos">Home</a> | <a href="/dojos/<%= @Dojo.id %>/students/new">Add Student</a>