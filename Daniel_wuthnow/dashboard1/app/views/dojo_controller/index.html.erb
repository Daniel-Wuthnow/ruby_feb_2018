<h1>Listing 4 Dojos</h1>
<table>
	<tr>
		<th>Branch</th>
		<th>Street</th>
		<th>City</th>
		<th>State</th>
		<th>Actions</th>
	</tr>
	<% @Dojos.each do |dojo| %>
	<tr>
	  <td><%= dojo.branch %></td>
	  <td><%= dojo.street %></td>
	  <td><%= dojo.city %></td>
	  <td><%= dojo.state %></td>
	  <td><a href="/dojos/<%= dojo.id %>">Show</a> | <a href="/dojos/<%= dojo.id %>/edit">Edit</a> | <a href="/dojos/<%= dojo.id %>">Destroy</a></td>
	  </tr>
	<% end %>

</table>
<a href="/dojos/new">New Dojo</a>
